{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python@3.0.7",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.8" }
    }
  ],
  "routes": [
    {
      "src": "/service",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}