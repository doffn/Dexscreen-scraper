{
  "version": 2,
  "builds": [
    {
    "src": "api/main_code.py",
    "use": "@vercel/python@3.0.7",
    "config": { "maxLambdaSize": "15mb", "runtime": "python3.8" }
  }
  ],
  "routes": [
    {
      "src": "/service",
      "dest": "api/main_code.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/main_code.py"
    }
  ]
  
}
